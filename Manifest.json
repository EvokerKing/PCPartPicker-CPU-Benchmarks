{
  "manifest_version": 3,
  "name": "PCPartPicker CPU Benchmarks",
  "version": "0.1",

  "description": "Adds the CPUs benchmark scores to PCPartPicker",
  "icons": {"512": "icon.png"},

  "permissions": ["webNavigation"],
  "content_scripts": [{
    "matches": ["https://pcpartpicker.com/products/cpu/*"],
    "css": ["style.css"],
    "js": ["main.js"]
  }]
}